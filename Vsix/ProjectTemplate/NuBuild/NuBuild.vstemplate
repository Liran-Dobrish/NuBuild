<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" Type="Project" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
   <TemplateData>
      <Name>NuGet Package</Name>
      <Description>NuGet package project</Description>
      <Icon>NuBuild.ico</Icon>
      <ProjectType>NuGet</ProjectType>
      <DefaultName>Package</DefaultName>
      <ProvideDefaultName>true</ProvideDefaultName>
   </TemplateData>
   <TemplateContent>
      <Project File="NuBuild.nuproj" ReplaceParameters="true">
         <ProjectItem ReplaceParameters="true" TargetFileName="$projectname$.nuspec">NuBuild.nuspec</ProjectItem>
      </Project>
   </TemplateContent>
</VSTemplate>
